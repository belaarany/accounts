{
	"rules": {
		"semicolon": [true, "never"],
		"indent": [true, "tabs", 4],
		"trailing-comma": [
			true,
			{
				"multiline": {
					"objects": "always",
					"arrays": "always",
					"functions": "never",
					"typeLiterals": "ignore"
				}
			}
		],
		"no-namespace": false,
		"no-inferrable-types": false,
		"object-literal-shorthand": false,
		"no-input-rename": false
	}
}
